generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  accountType String
  email       String   @unique(map: "email")
  interests   String[]
  password    String
  terms       Boolean
  username    String   @unique(map: "username")
}
